int n=8;
void setup() {
  Serial.begin(9600);
   pinMode(LED1, OUTPUT);
   pinMode(LED2, OUTPUT);
   pinMode(LED3, OUTPUT);
   pinMode(LED4, OUTPUT);
   pinMode(LED5, OUTPUT);
   pinMode(LED6, OUTPUT);
   pinMode(LED7, OUTPUT);
   pinMode(LED8, OUTPUT);
   
}

void disp()
{
 int i;
for(i=0;i<n;++i)
{
 
if(digitalRead(led_b[i]) == 1)
{
Serial.print('*');
}
else //if(digitalRead(led_b[i] == LOW))
{
Serial.print(0);
}
}
Serial.println();  
}

